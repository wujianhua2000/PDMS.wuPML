!entity 	= !!CE
!ncylist 	= !entity.members[1].members

!nsize = !ncylist.size()

!swriter = object FILE( 'd:\pml\plan-loop.txt' )
!swriter.open( 'WRITE' )

do  !nname  values  !ncylist

	!cyln1 = !nname.DBREF()
	!post = !cyln1.position.wrt( world )

	!swriter.writerecord( !post.east.string()  + ',' + !post.north.string() + ',' + !post.up.string()	 )
	
enddo

!swriter.close( )
