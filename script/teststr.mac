
!entCE			= !!CE

!enttype = !entCE.type

!ISOK = FALSE
if ( !enttype  EQ  'CMPF' ) then
	!ISOK = true
endif

q var !ISOK

!mark1 = !entCE.NAME.after( '-CE-' )
!mark2 = !entCE.NAME.before( '-OP' )

!NAME = !entCE.NAME

Q VAR !NAME  !MARK1  !MARK2

!entCE	=	!entCE.owner

!mark1 = !entCE.NAME.after( '-CE-' )
!mark2 = !entCE.NAME.before( '-OP' )

!NAME = !entCE.NAME

Q VAR !NAME  !MARK1  !MARK2
